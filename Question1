countof5 = 0
countof15 = 0
for i in range (1, 1000):
    if (i % 3 == 0):
        count1 += 1
    if (i % 5 == 0):
        count2 += 1
    if (i % 15 == 0):
        count3 += 1
print(count1)
print(count2)
print(count3)


countof3=3*(1+count1)*count1*1/2
print(countof3)
countof5=5*(1+count2)*count2*1/2
print(countof5)
countof15=15*(1+count3)*count3*1/2
print(countof15)
   

totalcount = countof3 + countof5 - countof15
print(totalcount)
# Answer is 233168
